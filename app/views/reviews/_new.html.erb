<p>Make a new review below:</p>


<%= form_with(model:[@cocktail_bar, Review.new]) do |f| %>
  <%= f.label "title:" %>
  <%= f.text_field :title %>
  <br>
  <%= f.label "comment:" %>
  <%= f.text_area :comment %>
  <br>
  <%= f.label "rating:" %>
  <%= f.number_field :rating, step: 0.5 %>
  <br>
  <%= f.submit "Submit" %>

<% end %>
